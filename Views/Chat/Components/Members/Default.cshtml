@model List<User>;

<h1>Участники чата</h1>
<div class="list-users g-list-style">
    @{
        #nullable disable
        foreach (User user in Model)
        {
            <div class="item">
                <div class="photo"><img src="~/images/Yura.png" alt=""></div>
                <a id="login-@user.Login" href="~/Account/Profile/@user.UserId">
                    <span class="username">@user.Login</span>
                    <span class="username">@user.FirstName</span>
                    <span class="username">@user.SecondName</span>
                    <span class="username status offline">offline</span>
                </a>
            </div>
        }
    }
</div>